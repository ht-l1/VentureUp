                                    <div class="location"><%= content.location %></div>
                                    <div class="hikedAt"><%= content.hikedAt %></div>
                                    <div class="rating">
                                    <% for(let i = 0; i < content.rating; i++) {%>
                                        <i class="fas fa-star"></i>
                                    <% }%>
                                    </div>
                                    <div class="details">
                                        <a href="<%= content.details.link %>"><%= content.details.text %></a>
